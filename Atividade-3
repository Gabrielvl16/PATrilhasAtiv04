using System;

namespace c
{
	class Program
	{
		public static void Main()
		{
			Console.WriteLine("Digite sua idade: ");
			int idade = int.Parse(Console.ReadLine());
			
			Console.WriteLine("Você possui um diploma de ensino superior? ");
			string simDiploma = Console.ReadLine().ToLower();
			
			if (idade >= 21 && simDiploma == "sim")
			{
				Console.WriteLine("Parabéns, você está qualificado para a vaga!");
			}
			else
			{
				Console.WriteLine("Você não esta qualificado para a vaga!");
			}
			
			Console.Write("Press any key to continue . . . ");
			Console.ReadKey(true);
		}
	}
}
